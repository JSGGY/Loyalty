{
    "version": 2,
    "builds": [
      {
        "public": "public/server.js",
        "use": "@vercel/node",
        "config": {
          "installCommand": "npm install && npm run build"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/public/server.js"
      }
    ]
  }
  